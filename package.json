{
  "name": "@kawaz/storage-client",
  "version": "1.0.0",
  "description": "Storage client library for Kawaz Plus services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "build:advanced": "npm run clean:advanced && npm i && tsc",
    "clean": "rimraf dist",
    "clean:advanced": "rimraf dist && rimraf node_modules && rimraf package-lock.json",
    "package": "npm run build:advanced && npm pack"
  },
  "keywords": [
    "storage",
    "s3",
    "aws"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/lib-storage": "^3.985.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "rimraf": "^6.0.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "registry": "https://your-artifactory-url/artifactory/api/npm/npm"
  }
}
